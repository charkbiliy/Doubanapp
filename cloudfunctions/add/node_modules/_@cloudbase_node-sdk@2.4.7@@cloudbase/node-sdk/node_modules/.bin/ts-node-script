#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../../_ts-node@8.10.2@ts-node/dist/bin-script.js" "$@"
  ret=$?
else 
  node  "$basedir/../../../../_ts-node@8.10.2@ts-node/dist/bin-script.js" "$@"
  ret=$?
fi
exit $ret
